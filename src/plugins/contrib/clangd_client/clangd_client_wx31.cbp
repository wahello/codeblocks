					<!-- <Add before="cmd /c @echo TARGET_OUTPUT_DIR: $(TARGET_OUTPUT_DIR)" /> -->
					<!-- debugging - <Add before="cmd /c echo CODEBLOCKS: $(CODEBLOCKS)" /> -->
					<!-- debugging - <Add before="cmd /c echo TARGET_DEVEL_DIR: $(TARGET_DEVEL_DIR)" /> -->
					<!-- debugging - <Add before="cmd /c echo PROJECT_DIR: $(PROJECT_DIR)" /> -->
					<!-- debugging - <Add before="g++ --version" /> -->

					<Add after="cmd /c if not exist $(TARGET_OUTPUT_DIR) mkdir $(TARGET_OUTPUT_DIR)" />

					<!-- Create the clangd_client.zip file -->
					<Add after="cmd /c if exist clangd_client.zip del clangd_client.zip" />
					<Add after="zip -jq9 clangd_client.zip src\resources\manifest.xml src/resources/*.xrc" />
					<Add after='cmd /c "cd src\resources &amp;&amp; zip -rq9 ..\..\clangd_client.zip images"' />

					<!-- Create the clangd_client.cbplugin file using the stripped dll -->
					<!-- clangd_client.cbplugin is NOT BUILT BY DEFAULT as requested by PECAN
						<Add after="cmd /c if exist clangd_client.cbplugin del clangd_client.cbplugin" />
						<Add after="cmd /c copy bin\clangd_client.dll" />
						<Add after="strip clangd_client.dll" />
						<Add after="zip -jq9 clangd_client.cbplugin clangd_client.dll  clangd_client.zip  documentation-install/*.txt" />
						<Add after="cmd /c del clangd_client.dll" />
					-->

					<Mode after="always" />
				</ExtraCommands>
				<Environment>
					<Variable name="TARGET_DEVEL_DIR_AC" value="D:\Andrew_Development\Work_Installers\AC-WindowsInstaller" />
					<Variable name="TARGET_DEVEL_DIR" value="$(CODEBLOCKS)\..\.." />
				</Environment>
